{
  "name": "templatejs-loader",
  "version": "3.1.3-alpha.0",
  "description": "template.js的webpakc编译loader",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "sideEffects": false,
  "exports": {
    "node": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    },
    "default": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.js"
    }
  },
  "types": "./types/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist ./types",
    "test": "nyc mocha",
    "release": "npm test && npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags",
    "lint": "eslint -c .eslintrc.cjs 'src/**/*.ts'",
    "lint:fix": "eslint --fix -c .eslintrc.cjs 'src/**/*.ts' --fix",
    "build:self": "rollup -c config/rollup.config.cjs",
    "build:esm": "rollup -c config/rollup.config.esm.cjs",
    "build": "npm run clean && npm run build:self && npm run build:esm",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "start": "http-server -p 3000 -c-1",
    "ci": "commit",
    "cz": "git-cz",
    "build:aio": "rollup -c config/rollup.config.aio.cjs"
  },
  "author": "yanhaijing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/yanhaijing/template.js.git"
  },
  "bugs": {
    "url": "https://github.com/yanhaijing/template.js/issues"
  },
  "files": [
    "/dist",
    "/types",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "keywords": [
    "template",
    "template.js",
    "js",
    "webpack"
  ],
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@commitlint/cz-commitlint": "^16.3.0",
    "@commitlint/format": "^12.1.1",
    "@commitlint/prompt-cli": "^16.3.0",
    "@js-lib/cli": "^2.3.3",
    "@jsmini/extend": "^0.5.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "babel-plugin-istanbul": "^6.1.1",
    "commitizen": "^4.2.4",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "es5-shim": "^4.6.7",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "expect.js": "^0.3.1",
    "http-server": "^14.1.1",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "3.1.0",
    "rimraf": "^5.0.5",
    "rollup": "^3.29.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@jsmini/extend": "^0.5.0",
    "@templatejs/precompiler": "^3.1.3-alpha.0",
    "@types/loader-utils": "^1.1.3",
    "loader-utils": "^1.2.3"
  },
  "peerDependencies": {
    "@templatejs/runtime": "^3.1.4"
  },
  "gitHead": "cebd8b4d696d9f8ed39c45bef68f79c382cd0389",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
