{
  "check-coverage": true,
  "lines": 0,
  "statements": 0,
  "functions": 0,
  "branches": 0,
  "require": [
    "ts-node/register",
    "source-map-support/register"
  ],
  "reporter": [
    "lcov",
    "text"
  ],
  "extension": [
    ".ts"
  ],
  "include": [
    "src/**.ts"
  ],
  "exclude": [
    "**/*.d.ts"
  ],
  "sourceMap": true,
  "instrument": true,
  "all": true
}
